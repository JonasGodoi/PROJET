[32m+[m
[32m+[m[32mimport java.net.URI;[m
[32m+[m[32mimport java.util.List;[m
[32m+[m
[32m+[m[32mimport org.springframework.beans.factory.annotation.Autowired;[m
[32m+[m[32mimport org.springframework.http.ResponseEntity;[m
[32m+[m[32mimport org.springframework.validation.annotation.Validated;[m
[32m+[m[32mimport org.springframework.web.bind.annotation.DeleteMapping;[m
[32m+[m[32mimport org.springframework.web.bind.annotation.GetMapping;[m
[32m+[m[32mimport org.springframework.web.bind.annotation.PathVariable;[m
[32m+[m[32mimport org.springframework.web.bind.annotation.PostMapping;[m
[32m+[m[32mimport org.springframework.web.bind.annotation.PutMapping;[m
[32m+[m[32mimport org.springframework.web.bind.annotation.RequestBody;[m
[32m+[m[32mimport org.springframework.web.bind.annotation.RequestMapping;[m
[32m+[m[32mimport org.springframework.web.bind.annotation.RestController;[m
[32m+[m[32mimport org.springframework.web.servlet.support.ServletUriComponentsBuilder;[m
[32m+[m
[32m+[m[32mimport com.example.gcvas.models.Agendar;[m
[32m+[m[32mimport com.example.gcvas.service.AgendarService;[m
[32m+[m
[32m+[m[32mimport jakarta.validation.Valid;[m
[32m+[m
[32m+[m[32m@RestController[m
[32m+[m[32m@RequestMapping("/agendar")[m
[32m+[m[32m@Validated[m
[32m+[m[32mpublic class AgendarController {[m
[32m+[m
[32m+[m[32m    @Autowired[m
[32m+[m[32m    AgendarService agendarService;[m
[32m+[m
[32m+[m[32m    @GetMapping[m
[32m+[m[32m    public ResponseEntity<List<Agendar>> getAgendar() {[m
[32m+[m[32m        return ResponseEntity.ok().body(agendarService.findAll());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @GetMapping("/{id}")[m
[32m+[m[32m    public ResponseEntity<Agendar> getAgendar(@PathVariable("id") Long id) {[m
[32m+[m[32m        Agendar obj = this.agendarService.findByid(id);[m
[32m+[m
[32m+[m[32m        return ResponseEntity.ok().body(obj);[m
[32m+[m[32m    }[m
[32m+[m[32m    @PostMapping[m
[32m+[m[32m    public ResponseEntity<Void> postAgendar(@RequestBody @Valid Agendar obj) {[m
[32m+[m[32m        this.agendarService.create(obj);[m
[32m+[m
[32m+[m[32m        URI uri = ServletUriComponentsBuilder.fromCurrentRequest().path("/{id}").buildAndExpand(obj.getId()).toUri();[m
[32m+[m
[32m+[m[32m        return ResponseEntity.created(uri).build();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m     @PutMapping("/{id}")[m
[32m+[m[32m    public ResponseEntity<Void> putAgendar(@PathVariable("id") Long id, @RequestBody Agendar newObj) {[m
[32m+[m[32m        newObj.setId(id);[m
[32m+[m
[32m+[m[32m        this.agendarService.update(newObj);[m
[32m+[m[41m        [m
[32m+[m[32m        return ResponseEntity.noContent().build();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @DeleteMapping("/{id}")[m
[32m+[m[32m    public ResponseEntity<Void> deleteBeneficiarioById(@PathVariable("id")Long id){[m
[32m+[m[32m        this.agendarService.deleteByid(id);[m
[32m+[m
[32m+[m[32m        return ResponseEntity.noContent().build();[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m}[m
