[32m+[m[32mimport React, { useEffect, useState } from 'react';[m
[32m+[m[32mimport api from '../../api/api'; // Certifique-se de que o cliente da API esteja configurado corretamente[m
[32m+[m
[32m+[m[32mconst HistoricoCRAS = () => {[m
[32m+[m[32m  const [historyData, setHistoryData] = useState([]);[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    // Fun√ß√£o para buscar os dados do backend[m
[32m+[m[32m    const fetchHistory = async () => {[m
[32m+[m[32m      try {[m
[32m+[m[32m        const response = await api.get('/Rma'); // Endpoint correto para buscar os dados do hist√≥rico[m
[32m+[m[32m        setHistoryData(response.data);[m
[32m+[m[32m      } catch (error) {[m
[32m+[m[32m        console.error('Erro ao buscar o hist√≥rico:', error);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    fetchHistory();[m
[32m+[m[32m  }, []);[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div>[m
[32m+[m[32m      <h2>Hist√≥rico de Registros do CRAS</h2>[m
[32m+[m[32m      <table>[m
[32m+[m[32m        <thead>[m
[32m+[m[32m          <tr>[m
[32m+[m[32m            <th>Unidade</th>[m
[32m+[m[32m            <th>N¬∫ Unidade</th>[m
