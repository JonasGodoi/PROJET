[31m-export function EditModal({ show, handleClose, selectedItem }) {[m
[32m+[m[32m// Modal para editar um registro existente[m
[32m+[m[32mexport function EditModal({ show, handleClose, selectedItem, handleSave }) {[m
[32m+[m[32m  const [codReq, setCodReq] = useState("");[m
[32m+[m[32m  const [desc_req, setdesc_req] = useState("");[m
[32m+[m[32m  const [status, setStatus] = useState("");[m
[32m+[m[32m  const [codNIS, setCodNIS] = useState("");[m
[32m+[m[32m  const [codBeneficio, setCodBeneficio] = useState("");[m
[32m+[m[32m  const [date, setDate] = useState("");[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    // Atualiza os estados quando o item selecionado mudar[m
[32m+[m[32m    if (selectedItem) {[m
[32m+[m[32m      setCodReq(selectedItem.codReq || "");[m
[32m+[m[32m      setdesc_req(selectedItem.desc_req || "");[m
[32m+[m[32m      setStatus(selectedItem.status || "");[m
[32m+[m[32m      setCodNIS(selectedItem.codNIS || "");[m
[32m+[m[32m      setCodBeneficio(selectedItem.codBeneficio || "");[m
[32m+[m[32m      setDate(selectedItem.date || "");[m
[32m+[m[32m    }[m
[32m+[m[32m  }, [selectedItem]);[m
[32m+[m
[32m+[m[32m  const onSave = () => {[m
[32m+[m[32m    const requisicaoAtualizada = {[m
[32m+[m[32m      codReq,[m
[32m+[m[32m      desc_req,[m
[32m+[m[32m      status,[m
[32m+[m[32m      codNIS,[m
[32m+[m[32m      codBeneficio,[m
