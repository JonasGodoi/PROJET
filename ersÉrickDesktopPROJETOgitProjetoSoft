[32m+[m[32m    }));[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleSubmit = async (e) => {[m
[32m+[m[32m    e.preventDefault(); // Evita o comportamento padr√£o do formul√°rio[m
[32m+[m[32m    setLoading(true);[m
[32m+[m[32m    setError(null);[m
[32m+[m[32m    setSuccess(false);[m
[32m+[m
[32m+[m[32m    const payload = {[m
[32m+[m[32m      username: formData.nome,[m
[32m+[m[32m      cpf: formData.cpf,[m
[32m+[m[32m      telefone: formData.telefone,[m
[32m+[m[32m      endereco: formData.endereco,[m
[32m+[m[32m      data: formData.dataNascimento, // Formato: 'yyyy-MM-dd'[m
